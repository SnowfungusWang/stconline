  <process id="Contract" isClosed="false" isExecutable="true" processType="None">
    <startEvent id="start" name="开始">
      <extensionElements>
        <activiti:formProperty id="ContractID" name="ID of contract" required="true" type="String"/>
      </extensionElements>
    </startEvent>
    <endEvent id="end" name="结束"/>
    <parallelGateway gatewayDirection="Unspecified" id="mApproveForkGate" name="Fork"/>
    <userTask activiti:exclusive="true" id="mmContractReview" name="MMReviewContract"/>
    <userTask activiti:exclusive="true" id="qmContractReview" name="QMReviewContract"/>
    <parallelGateway gatewayDirection="Unspecified" id="mApproveJoinGate" name="Join"/>
