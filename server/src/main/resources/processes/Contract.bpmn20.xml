    </sequenceFlow>
    <sequenceFlow id="_10" name="通过" sourceRef="MMApproveGate" targetRef="mApproveJoinGate">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewContractResult == 'ReviewPass'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_11" name="通过" sourceRef="QMApproveGate" targetRef="mApproveJoinGate">
