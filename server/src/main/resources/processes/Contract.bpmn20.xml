    <sequenceFlow id="_12" name="不通过" sourceRef="QMApproveGate" targetRef="submitContract">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewContractResult == 'ReviewDisprove'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_2" sourceRef="start" targetRef="submitContract"/>
    <sequenceFlow id="_3" sourceRef="submitContract" targetRef="mApproveForkGate"/>
