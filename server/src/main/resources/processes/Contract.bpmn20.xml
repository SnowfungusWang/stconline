      <extensionElements>
        <activiti:formProperty id="reviewContractResult" name="Do you approve this request?" required="true" type="String"/>
        <activiti:formProperty id="user" name="client" type="string" value="client"/>
        <activiti:formProperty id="reviewContractComment" name="reason about pass or reject" required="true" type="string"/>
      </extensionElements>
    </userTask>
    <exclusiveGateway gatewayDirection="Unspecified" id="cusApproveGate" name="CUS是否通过"/>
    <userTask activiti:assignee="${ClientID}" activiti:exclusive="true" id="submitContract" name="cusReviewContract">
      <extensionElements>
        <activiti:formProperty id="user" name="client" type="string" value="client"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="fStartToFork" sourceRef="start" targetRef="mApproveForkGate"/>
    <sequenceFlow id="fForkToMM" sourceRef="mApproveForkGate" targetRef="mmContractReview"/>
    <sequenceFlow id="fForkToQM" sourceRef="mApproveForkGate" targetRef="qmContractReview"/>
    <sequenceFlow id="fSubmitToFork" sourceRef="submitContract" targetRef="mApproveForkGate"/>
    <sequenceFlow id="fMMToJoin" sourceRef="mmContractReview" targetRef="mApproveJoinGate"/>
    <sequenceFlow id="fQMToJoin" sourceRef="qmContractReview" targetRef="mApproveJoinGate"/>
    <sequenceFlow id="fJoinToCus" sourceRef="mApproveJoinGate" targetRef="cusContractReview"/>
    <sequenceFlow id="fCusToCusApprove" sourceRef="cusContractReview" targetRef="cusApproveGate"/>
    <sequenceFlow id="fCusApproveToEnd" name="通过" sourceRef="cusApproveGate" targetRef="end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewEntrustResult == 'ReviewPass'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="fCusApproveToSubmit" name="不通过" sourceRef="cusApproveGate" targetRef="submitContract">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewEntrustResult == 'ReviewDisprove'}]]></conditionExpression>
    </sequenceFlow>
  </process>
