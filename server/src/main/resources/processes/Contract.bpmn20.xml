  <process id="Contract" isClosed="false" isExecutable="true" name="合同" processType="None">
    <startEvent id="start" name="开始">
      <extensionElements>
        <activiti:formProperty id="ContractID" name="ID of contract" required="true" type="String"/>
      </extensionElements>
    </startEvent>
    <endEvent id="end" name="结束"/>
    <parallelGateway gatewayDirection="Unspecified" id="mApproveForkGate" name="Fork"/>
    <userTask activiti:exclusive="true" id="mmContractReview" name="MMReviewContract">
      <extensionElements>
        <activiti:formProperty id="reviewContractResult" name="Do you approve this request?" required="true" type="String"/>
        <activiti:formProperty id="user" name="MM" type="string" value="MarketingManager"/>
        <activiti:formProperty id="reviewContractComment" name="reason about pass or reject" required="true" type="string"/>
      </extensionElements>
    </userTask>
    <userTask activiti:exclusive="true" id="qmContractReview" name="QMReviewContract">
      <extensionElements>
        <activiti:formProperty id="reviewContractResult" name="Do you approve this request?" required="true" type="String"/>
        <activiti:formProperty id="user" name="QM" type="string" value="QualityManager"/>
        <activiti:formProperty id="reviewContractComment" name="reason about pass or reject" required="true" type="string"/>
      </extensionElements>
    </userTask>
    <parallelGateway gatewayDirection="Unspecified" id="mApproveJoinGate" name="Join"/>
    <userTask activiti:assignee="${ClientID}" activiti:exclusive="true" id="cusContractReview" name="CusReviewContract">
      <extensionElements>
        <activiti:formProperty id="reviewContractResult" name="Do you approve this request?" required="true" type="String"/>
        <activiti:formProperty id="user" name="client" type="string" value="client"/>
        <activiti:formProperty id="reviewContractComment" name="reason about pass or reject" required="true" type="string"/>
      </extensionElements>
    </userTask>
    <exclusiveGateway gatewayDirection="Unspecified" id="cusApproveGate" name="CUS是否通过"/>
    <userTask activiti:exclusive="true" id="submitContract" name="WorkerSubmitContract">
      <extensionElements>
        <activiti:formProperty id="user" name="worker" type="string" value="worker"/>
      </extensionElements>
    </userTask>
    <sequenceFlow id="fForkToMM" sourceRef="mApproveForkGate" targetRef="mmContractReview"/>
    <sequenceFlow id="fForkToQM" sourceRef="mApproveForkGate" targetRef="qmContractReview"/>
    <sequenceFlow id="fCusToCusApprove" sourceRef="cusContractReview" targetRef="cusApproveGate"/>
    <sequenceFlow id="fCusApproveToEnd" name="通过" sourceRef="cusApproveGate" targetRef="end">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewContractResult == 'ReviewPass'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="fCusApproveToSubmit" name="不通过" sourceRef="cusApproveGate" targetRef="submitContract">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewContractResult == 'ReviewDisprove'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="fStartToSubmit" sourceRef="start" targetRef="submitContract"/>
    <sequenceFlow id="fSubmitToFork" sourceRef="submitContract" targetRef="mApproveForkGate"/>
    <sequenceFlow id="fMMReviewToJoin" sourceRef="mmContractReview" targetRef="mApproveJoinGate"/>
    <sequenceFlow id="fQMReviewToJoin" sourceRef="qmContractReview" targetRef="mApproveJoinGate"/>
    <exclusiveGateway gatewayDirection="Unspecified" id="managerApproveGate" name="ExclusiveGateway"/>
    <sequenceFlow id="fJoinToGate" sourceRef="mApproveJoinGate" targetRef="managerApproveGate"/>
    <sequenceFlow id="fGateToCusReview" name="通过" sourceRef="managerApproveGate" targetRef="cusContractReview">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewContractResult == 'ReviewPass'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="fGateToSubmit" name="不通过" sourceRef="managerApproveGate" targetRef="submitContract">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewContractResult == 'ReviewDisprove'}]]></conditionExpression>
    </sequenceFlow>
  </process>
